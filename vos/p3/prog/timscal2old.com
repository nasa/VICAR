 900  FORMAT(//'  CURRENT GEOMETRY DIMENSION ',I5 ,/                    
     X,' JMAXST = ',I5,' RESET AVTRAT TDIFF1 TDIFF2 TO 2. 10. 20.')     
      STOP 'GEO :JMAXST .GT. MAXGEO'                                    
      ENDIF                                                             
      DO 350 IK=1,JMAXST                                                
