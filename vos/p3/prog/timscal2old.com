      IF(CTHIK.NE.0) IFLGT=1                                            
      IF(CALT.NE.0)  IFLGA=1                                            
      IF(ISEED.EQ.0) IFLGT=2                                            
      IF(ISEED.EQ.0) IFLGA=2                                            
