      IRD1 = 0                                                          
      IRD2 = 0                                                          
      IF (MODEL.EQ.0) LEN = 0                                           
      IF((MODEL.EQ.0) .OR. (MODEL.EQ.7)) GO TO 110                      
      IF(M1.EQ.0) M1=MODEL                                              
      IF(M2.EQ.0) M2=MODEL                                              
      IF(M3.EQ.0) M3=MODEL                                              
      IF(M4.EQ.0) M4=MODEL                                              
      IF(M5.EQ.0) M5=MODEL                                              
      IF(M6.EQ.0) M6=MODEL                                              
      IF(MDEF.EQ.0) MDEF=1                                              
110   CONTINUE                                                          
      M=MODEL                                                           
