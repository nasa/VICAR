1200  FORMAT(4I5)                                                       
      DO IK=1,4                                                         
          IREG(IK) = IREGX(IK)                                          
      END DO                                                            
Calley      WRITE(IPR,1210) (IREG(IK),IK=1,4)                           
      IF (IPR .GT. 0) WRITE(IPR,1210) (IREG(IK),IK=1,4)                 
1210  FORMAT('0 CARD 2D *****',4I5)                                     
