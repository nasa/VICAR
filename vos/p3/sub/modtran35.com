Calley     1      H1,H2,ANGLE,IDAY,RO,ISOURC,ANGLEM                     
          H1 = H1X                                                      
          H2 = H2X                                                      
          ANGLE = ANGLEX                                                
          IDAY = IDAYX                                                  
          RO = ROX                                                      
          ISOURC = ISOURCX                                              
          ANGLEM = ANGLEMX                                              
Calley          WRITE(IPR,'(A,2(3F10.5,I5,F15.5,I5))')                  
          IF (IPR .GT. 0) WRITE(IPR,'(A,2(3F10.5,I5,F15.5,I5))')        
     1      '0 CARD 3   *****',H1,H2,ANGLE,IDAY,RO,ISOURC,ANGLEM        
          ITYPE=3                                                       
          RANGE=0.                                                      
          BETA=0.                                                       
          LENN=0                                                        
          PHI=0.                                                        
      ENDIF                                                             
C                                                                       
