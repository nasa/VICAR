          ABSC(IHC,I) = ABSCX(IHC,I)                                    
          ASYM(IHC,I) = ASYMX(IHC,I)                                    
      END DO                                                            
Calley      WRITE(IPR,'((2X,3(F6.2,2F7.5,F6.4)))')                      
      IF (IPR .GT. 0) WRITE(IPR,'((2X,3(F6.2,2F7.5,F6.4)))')            
     1  (VX(I),EXTC(IHC,I),ABSC(IHC,I),ASYM(IHC,I),I=1,NWAVLN)          
1300  CONTINUE                                                          
      RETURN                                                            
      END                                                               
