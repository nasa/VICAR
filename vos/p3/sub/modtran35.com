      RATES(K)=RATTAB(KJ)                                               
      DO 24 J=1,JMAX                                                    
      IJ = ILOW + J                                                     
      Y(J)= ALOG(ATTAB(IJ,KJ))                                          
   24 CONTINUE                                                          
      ATTN(K)=EXP(AITK(X,Y,FREQ,JMAX) )                                 
   25 CONTINUE                                                          
C        APPLY TEMPERATURE CORRECTION                                   
      DO 31 I=2,5                                                       
      IF(T.LT.TMPTAB(I)) GO TO 33                                       
   31 CONTINUE                                                          
      ILOW = 4                                                          
      GO TO 35                                                          
   33 ILOW = I-1                                                        
   35 CONTINUE                                                          
      DO 41 J=2,8                                                       
