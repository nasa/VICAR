      SUBROUTINE SINT(V1,V1C,DV,NPT,CONTI,CONTO)                        
C                                                                       
C     INTERPOLATION  FOR CONTINUUM WITH LOWTRAN                         
C                                                                       
      DIMENSION CONTI(2003)                                             
      CONTO=0.                                                          
      I=(V1C-V1)/DV+1.00001                                             
      IF(I.GE.NPT)GO TO 10                                              
      CONTO=CONTI(I)                                                    
      IMOD=AMOD(V1C,10.)                                                
      IF(IMOD.GT.0) CONTO=(CONTI(I)+CONTI(I+1))/2.                      
10    CONTINUE                                                          
      RETURN                                                            
      END                                                               
      SUBROUTINE SLF260(V1C,SH2OT1)                                     
