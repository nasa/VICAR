write " (45.0,2.0) -> (6.4099229,192.81305)  p_1=35 degrees"
write "Note that above tests are with radii=6380."
write " "
let $echo="yes"
let $becho="yes"

tmp_routines +
	LATITUDES=(45.0,75.0,60.0,50.0,40.0,35.0)	+
	LONGITUDES=(2.0,80.0,10.0,30.0,40.0,75.0)	+
